<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.cell.*?> 
<?import javafx.collections.*?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Label?>
<?import hu.elte.bfw1p6.poker.model.entity.PokerTable?>
<?import javafx.scene.control.Button?>


<GridPane alignment="CENTER" hgap="10.0" vgap="10.0"
	xmlns:fx="http://javafx.com/fxml" fx:controller="hu.elte.bfw1p6.poker.client.controller.UserListerController">
	<padding>
		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
	</padding>
	<Label style="-fx-font: NORMAL 20 Tahoma;" text="Regisztrált felhasználók"
		GridPane.columnIndex="0" GridPane.rowIndex="0">
	</Label>
	<TableView fx:id="userView" GridPane.columnIndex="0"
		GridPane.rowIndex="1">
		<columns>
			<TableColumn prefWidth="100.0" text="Username"
				fx:id="userName" />
			<TableColumn prefWidth="100.0" text="Regisztráció dátuma"
				fx:id="regDate" />
			<TableColumn prefWidth="100.0" text="Egyenleg"
				fx:id="balance" />
		</columns>
	</TableView>
	<Button fx:id="backButton" text="Vissza" onAction="#handleBack"
		GridPane.columnIndex="1" GridPane.rowIndex="1" />
</GridPane>